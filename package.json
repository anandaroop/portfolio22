{
  "name": "portfolio22",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "graphql": "json-graphql-server data/data.json --p 3001",
    "dev": "next dev",
    "dark": "while [ true ]; do osascript -e 'tell app \"System Events\" to tell appearance preferences to set dark mode to not dark mode'; sleep 3; done",
    "lint": "next lint",
    "format": "prettier --write .",
    "clean": "rm -rf out .next",
    "clean-data": "find . -depth 1 -regex '^.*data\\..*\\.json$' | grep -v 'data.json' | xargs rm",
    "clean-images": "find public/slides -type f -not -path '*/orig/*' -print0 | xargs -0 rm",
    "images": "ts-node src/scripts/process-images.ts",
    "searchable-content": "ts-node src/scripts/generate-searchable-content.ts",
    "build": "next build",
    "preexport": "yarn clean && next build",
    "export": "next export",
    "serve": "serve out",
    "start": "next start",
    "deploy": "./src/scripts/deploy.sh",
    "go": "yarn export && yarn deploy"
  },
  "dependencies": {
    "minisearch": "^5.0.0",
    "next": "12.2.5",
    "photoswipe": "^5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-lazy-load-image-component": "^1.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@sindresorhus/slugify": "^2.1.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/lodash": "^4.14.184",
    "@types/node": "18.7.12",
    "@types/photoswipe": "^4.1.2",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/sharp": "^0.30.5",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "autoprefixer": "^10.4.8",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.0",
    "graphql-request": "^5.0.0",
    "husky": "^8.0.1",
    "json-graphql-server": "^2.3.2",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "serve": "^14.0.1",
    "sharp": "^0.30.7",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  }
}
